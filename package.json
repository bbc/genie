{
    "name": "genie",
    "version": "3.8.11",
    "description": "BBC Games Genie - A Modular Game Engine",
    "license": "Apache-2.0",
    "private": true,
    "author": "BBC",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbc/genie"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-private-methods": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "ajv": "^6.12.6",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "bowser": "2.11.0",
        "core-js": "^3.8.3",
        "expose-loader": "^0.7.5",
        "json5": "^2.1.3",
        "notp": "^2.0.3",
        "phaser": "3.24.1",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "cypress": "^6.5.0",
        "cypress-plugin-tab": "^1.0.5",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-local-rules": "^0.1.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.1.0",
        "jest": "^26.6.3",
        "jest-canvas-mock": "^2.3.0",
        "jsdoc": "^3.6.6",
        "jsdom": "^16.4.0",
        "local-web-server": "^4.2.1",
        "minimist": "^1.2.5",
        "mochawesome": "^6.2.1",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^5.1.0",
        "prettier": "2.1.2",
        "rollup": "^2.35.1",
        "rollup-plugin-terser": "^7.0.2",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.1",
        "webpack-visualizer-plugin": "^0.1.11"
    },
    "scripts": {
        "clean": "rm -rf .cache",
        "create-theme2": "bash build-scripts/theme2creator/create.sh",
        "start": "cross-env genieCore=true ws --config-file dev/scripts/web-config.js",
        "start:pack": "webpack-dev-server -d --config build-scripts/webpack.config.js --env.genieCore",
        "build": "npm run create-theme2 && webpack --config build-scripts/webpack.config.js --env.genieCore",
        "eslint": "eslint . --ignore-pattern lib/ --ignore-path .gitignore",
        "jest": "jest --coverage",
        "jest:no-coverage": "jest",
        "test": "npm run jest && npm run eslint",
        "docs": "jsdoc -c ./build-scripts/docs/.jsdoc.js",
        "validate": "node build-scripts/schemavalidator/validator.js achievement",
        "roll-lodash": "rollup -c lib/lodash/rollup.config.js",
        "cy": "export THEME=theme_1 && export LOCAL_DEV=true && cypress open",
        "cy:headless": "export THEME=theme_1 && cypress run --headless --config video=false --spec '**/*spec.js'",
        "cy:headless-theme2": "export THEME=theme_2 && cypress run --headless --config video=false --spec '**/*spec.js'",
        "cy:local": "export THEME=theme_1 && export LOCAL_DEV=true && cypress run --headless --browser chrome --config video=false --spec '**/*spec.js'",
        "cy:local-theme2": "export THEME=theme_2 && export LOCAL_DEV=true && cypress run --headless --browser chrome --config video=false --spec '**/*spec.js'"
    }
}
