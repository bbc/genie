{
    "name": "genie",
    "version": "3.16.0-pre3",
    "description": "BBC Games Genie - A Modular Game Engine",
    "license": "Apache-2.0",
    "private": true,
    "author": "BBC",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbc/genie"
    },
    "dependencies": {
        "@babel/core": "^7.22.10",
        "@babel/eslint-parser": "^7.22.10",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/preset-env": "^7.22.10",
        "ajv": "^6.12.6",
        "babel-jest": "^29.6.3",
        "babel-loader": "^9.1.3",
        "babel-plugin-module-resolver": "^5.0.0",
        "bowser": "^2.11.0",
        "core-js": "^3.32.1",
        "event-target-polyfill": "^0.0.3",
        "expose-loader": "^4.1.0",
        "json5": "^2.2.3",
        "notp": "^2.0.3",
        "phaser": "3.60.0",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.3",
        "@webpack-cli/serve": "^2.0.5",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "bufferutil": "^4.0.7",
        "cross-env": "^7.0.3",
        "cypress": "^7.7.0",
        "cypress-plugin-tab": "^1.0.5",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jest": "^27.2.3",
        "eslint-plugin-local-rules": "^1.3.2",
        "eslint-plugin-prettier": "^5.0.0",
        "games-stat-helper": "github:bbc/games-automation-stathelper.git",
        "jest": "^29.6.3",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.6.3",
        "local-web-server": "^5.3.0",
        "minimist": "^1.2.8",
        "mocha": "^10.0.0",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.2.1",
        "mochawesome-report-generator": "^6.2.0",
        "phaser3-rex-plugins": "github:/rexrainbow/phaser3-rex-notes.git",
        "prettier": "^3.0.1",
        "rollup": "^3.27.2",
        "utf-8-validate": "^6.0.3",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "scripts": {
        "start": "cross-env genieCore=true ws --config-file dev/scripts/web-config.js",
        "start:pack": "webpack-cli serve --config build-scripts/webpack.config.js --env genieCore",
        "build": "webpack --config build-scripts/webpack.config.js --env genieCore",
        "build:theme2": "bash build-scripts/theme2creator/create.sh",
        "test": "npm run jest && npm run eslint",
        "eslint": "eslint . --ignore-path .gitignore",
        "jest": "jest --coverage",
        "jest:no-coverage": "jest",
        "validate": "node build-scripts/schemavalidator/validator.js achievement",
        "roll:lodash": "rollup -c lib/lodash/rollup.config.js",
        "roll:ninepatch": "rollup -c lib/rollup.ninepatch.config.js",
        "cy:terminal": "cypress open --env theme=theme_1,url=https://www.bbc.co.uk/childrens/assets/games/games-atlas/test/index.html",
        "cy:terminal:local": "cypress open --env theme=theme_1,url=http://localhost:9001/",
        "cy:headless": "cypress run --headless --config video=false --env theme=theme_1 url=https://www.bbc.co.uk/childrens/assets/games/games-atlas/test/index.html --spec '**/*spec.js'",
        "cy:headless-theme2": "cypress run --headless --config video=false --env theme=theme_2,url=https://www.bbc.co.uk/childrens/assets/games/games-atlas/test/index.html --spec '**/*spec.js'",
        "cy:local": "cypress run --headless --config video=false --env theme=theme_1,url=http://localhost:9001/ --spec '**/integration/*spec.js'",
        "cy:local-theme2": "cypress run --headless --config video=false --env theme=theme_2,url=http://localhost:9001/ --spec '**/integration/*spec.js'"
    }
}
