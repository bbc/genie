{
    "name": "genie",
    "version": "3.8.6",
    "description": "BBC Games Genie - A Modular Game Engine",
    "license": "Apache-2.0",
    "private": true,
    "author": "BBC",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbc/genie"
    },
    "dependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-private-methods": "^7.10.4",
        "@babel/preset-env": "^7.11.5",
        "ajv": "^6.12.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "bowser": "2.11.0",
        "core-js": "^3.6.5",
        "expose-loader": "^0.7.5",
        "json5": "^2.1.3",
        "phaser": "3.24.1",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-local-rules": "^0.1.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "jest": "^26.0.1",
        "jest-canvas-mock": "^2.2.0",
        "jsdoc": "^3.6.4",
        "jsdom": "^16.2.2",
        "local-web-server": "^4.0.0",
        "minimist": "^1.2.5",
        "prettier": "2.1.2",
        "rollup": "^2.25.0",
        "rollup-plugin-terser": "^7.0.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-visualizer-plugin": "^0.1.11"
    },
    "scripts": {
        "clean": "rm -rf .cache",
        "create-theme2": "bash build-scripts/theme2creator/create.sh",
        "start": "cross-env genieCore=true ws --config-file dev/scripts/web-config.js",
        "start:pack": "webpack-dev-server -d --config build-scripts/webpack.config.js --env.genieCore",
        "build": "npm run create-theme2 && webpack --config build-scripts/webpack.config.js --env.genieCore",
        "eslint": "eslint . --ignore-pattern lib/ --ignore-path .gitignore",
        "jest": "jest --coverage",
        "jest:no-coverage": "jest",
        "test": "npm run jest && npm run eslint",
        "docs": "jsdoc -c ./build-scripts/docs/.jsdoc.js",
        "validate": "node build-scripts/schemavalidator/validator.js achievement",
        "roll-lodash": "rollup -c lib/lodash/rollup.config.js"
    }
}
